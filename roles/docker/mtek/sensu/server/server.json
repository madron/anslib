{
  "rabbitmq": {
    "host": "rabbitmq",
    "port": 5672,
    "user": "guest",
    "password": "guest",
    "vhost": "/"
  },
  "redis": {
    "host": "redis",
    "port": 6379
  },
  "handlers": {
    "default": {
      "type": "set",
      "handlers": [
        "stdout"
      ]
    },
    "stdout": {
      "type": "pipe",
      "command": "cat"
    }
  },
  "checks": {
    "test": {
      "command": "echo -n OK",
      "subscribers": [
        "test-servers"
      ],
      "interval": 60
    }
  }
}
